{
  "name": "mlm-packaging-backend",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "dev:mysql": "cross-env NODE_ENV=development DB_TYPE=mysql nodemon server.js",
    "dev:postgres": "cross-env NODE_ENV=development DB_TYPE=postgres nodemon server.js",
    "setup:mysql": "node scripts/setup-databases.js --mysql",
    "setup:postgres": "node scripts/setup-databases.js --postgres",
    "setup:all": "node scripts/setup-databases.js --all",
    "prod": "cross-env NODE_ENV=production node server.js",
    "test:db": "node scripts/test-database.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.16.1",
    "pdfkit": "^0.14.0",
    "pg": "^8.17.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.11"
  }
}
